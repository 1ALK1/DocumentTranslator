<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.microsoft.com/MSNSearch/2005/09/fex" elementFormDefault="qualified" targetNamespace="http://schemas.microsoft.com/MSNSearch/2005/09/fex" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import schemaLocation="http://mtloc.live-int.com:84/translation?xsd=xsd1" namespace="http://schemas.microsoft.com/2003/10/Serialization/Arrays" />
